
import math
import os
import random
import re
import sys

def sockMerchant(n, ar):
    t = 0
    st = set(ar)
    for i in st:
        t += ar.count(i)//2
    print(t)
    

if __name__ == '__main__':
    n = int(input().strip())
    ar = list(map(int, input().rstrip().split()))
    result = sockMerchant(n, ar)
